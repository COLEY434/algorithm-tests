function subString(string1,string2){
  let obj = {}
  let i;
  let j;
  let result = true;
  
  for(i of string1)
  obj[i] = i


for(j of string2){
//console.log()
}
  if(obj[j]){
   return result
   }
   
  
  else{
    result = false
    return result
    }
    //console.log("No")
  
  

  return result
  

  
}
console.log(subString("hello","bano"))
